
local Window = OrionLib:MakeWindow({Name = "hubs n scripts", HidePremium = false, SaveConfig = true, introtext = "loading", ConfigFolder = "hubs n studs"})

local Tab = Window:MakeTab({
	Name = "home",
	Icon = "rbxassetid://16207214733",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "stop hub",
	Callback = function()
      		OrionLib:destroy()
  	end    
})

Tab:AddSlider({
	Name = "Walkspeed",
	Min = 16,
	Max = 1000,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(s)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
	end    
})

Tab:AddSlider({
	Name = "jump power",
	Min = 16,
	Max = 1000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(s)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
	end    
})


local Tab = Window:MakeTab({
	Name = "hubs",
	Icon = "rbxassetid://16207110677",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "astral hub",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/Vcsk/AstralHub/main/Main.lua", true))()
  	end

